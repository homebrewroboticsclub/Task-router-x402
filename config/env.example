HOST=0.0.0.0
PORT=3000

X402_PRIVATE_KEY=replace-with-private-key
X402_WALLET_ID=wallet-identifier
X402_GATEWAY_URL=https://api.corbits.dev
X402_PAYMENT_ENDPOINT=/v1/payments
X402_PAYMENT_TIMEOUT_MS=10000
X402_PAYMENT_PROVIDER=gateway
X402_CONFIRM_ATTEMPTS=5
X402_CONFIRM_DELAY_MS=2000
# Solana RPC: helius (recommended), public, or custom
SOLANA_RPC_PROVIDER=helius
HELIUS_API_KEY=your-helius-api-key
# Fallback RPC (avoid mainnet-beta â€” returns 403). Helius can be configured in admin panel or via HELIUS_API_KEY
X402_SOLANA_RPC_URL=https://solana-rpc.publicnode.com
X402_SOLANA_COMMITMENT=confirmed
X402_SOLANA_MIN_CONFIRMATIONS=1
# Base64 or JSON secret key; defaults to X402_PRIVATE_KEY when omitted
X402_SOLANA_SECRET_KEY=

COMMAND_DANCE_STRATEGY=lowest_price
COMMAND_BUY_COLA_STRATEGY=closest

PRICING_MARKUP_PERCENT=10

ROBOT_HEALTH_TIMEOUT_MS=5000
ROBOT_COMMAND_TIMEOUT_MS=8000
ROBOT_HEALTH_ENDPOINT=/health
ROBOT_SECURE_HEALTH_ENDPOINT=/helth

# Admin Panel Authentication
ADMIN_USERNAME=admin
ADMIN_PASSWORD=change-this-password

# AI Agent Configuration (for Task Router mode executor selection)
AI_AGENT_STRATEGY=smart
# Options: smart, lowest_price, closest, fastest
# N8N Integration (optional)
N8N_WEBHOOK_URL=
# Example: https://your-n8n-instance.com/webhook/robot-selection

